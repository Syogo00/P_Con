<div>
  <h1>所属部門変更</h1>
  <div>
    <%= form_with model: @member, url: admin_member_path(@member.id), method: :patch do |f| %>
      <div>
        <%= f.select :section_id, options_from_collection_for_select(Section.all, :id, :section_name) %>
      </div>
      <div>
        <%= f.submit "変更を保存" %>
      </div>
    <% end %>
  </div>
</div>